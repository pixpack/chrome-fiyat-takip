{
  "version": 2,
  "builds": [
    {
      "src": "server-vercel.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server-vercel.js"
    },
    {
      "src": "/webhook/(.*)",
      "dest": "server-vercel.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/check-prices",
      "schedule": "*/5 * * * *"
    }
  ]
}
